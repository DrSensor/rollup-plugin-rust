{
	"root": true,
	"parser": "babel-eslint",
	"plugins": ["import", "promise", "node", "prettier", "babel", "flowtype"],
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended",
		"plugin:flowtype/recommended",
		"prettier/flowtype"
	],
	"env": {
		"node": true,
		"es6": true
	},
	"rules": {
		"no-console": "off"
	}
}
